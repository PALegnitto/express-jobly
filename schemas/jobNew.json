{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 60
      },
      "salary": {
        "type": "integer",
        "minimum": 0,
        "maximum": 500000
      },
      "equity": {
        "type": "number",
        "minimum": 0,
        "maximum": 1.0
      },
      "companyHandle": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ]
  }